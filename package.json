        "lodash": "^4.17.4",
        "popper.js": "^1.12",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.23.6",
        "sass-loader": "^8.0.0",
        "vue": "^2.6.11",
        "vue-resource": "^1.5.1",
        "vue-router": "^3.1.6",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "bootstrap": "^4.3.1",
        "moment": "^2.24.0",
        "toastr": "^2.1.4",
        "vue-paginate": "^3.6.0"
    }
